{
  "manifest_version": 2,
  "name": "Post Internet",
  "version": "0.0.2",
  "description": "Postprocess the Internet",
  "icons": {
    "16": "images/16.png",
    "128": "images/128.png"
  },
  "author": "Takayosi Amagi <fand.gmork@gmail.com>",
  "background": {
    "persistent": false,
    "scripts": ["index.js"]
  },
  "permissions": ["activeTab", "storage"],
  "options_page": "options.html",
  "browser_action": {
    "default_icon": {
      "19": "images/19.png",
      "38": "images/38.png"
    },
    "default_title": "Post Internet"
  }
}
